<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:VerticalPanel ui:field="drivesPanel">
		<g:VerticalPanel ui:field="insertDiskPanel">
			<g:Label>Insert Disk</g:Label>
			<g:HorizontalPanel>
				<g:Label>File:</g:Label>
				<g:Label ui:field="selectedFile">select file...</g:Label>
				<g:HTML>&nbsp;</g:HTML>
				<g:Button ui:field="chooseButton">Choose...</g:Button>
			</g:HorizontalPanel>
			<g:HorizontalPanel>
				<g:Label>Drives: </g:Label>
				<g:ListBox ui:field="drivesListBox"></g:ListBox>
				<g:HTML>&nbsp;</g:HTML>
				<g:CheckBox ui:field="writeProtectCheckBox">Write Protect</g:CheckBox>
				<g:HTML>&nbsp;</g:HTML>
				<g:Button ui:field="insertDiskButton">Insert Disk</g:Button>
			</g:HorizontalPanel>
		</g:VerticalPanel>
		<g:FlexTable ui:field="fileSetGrid">
		</g:FlexTable>
		<g:HorizontalPanel>
			<g:Button ui:field="ejectButton">Eject</g:Button>
			<g:HTML>&nbsp;</g:HTML>
			<g:Button ui:field="writeProtectButton">Write Protect</g:Button>
			<g:HTML>&nbsp;</g:HTML>
			<g:Button ui:field="writeEnableButton">Write Enable</g:Button>
			<g:HTML>&nbsp;</g:HTML>
			<g:Button ui:field="openSetButton">Open Set</g:Button>
			<g:HTML>&nbsp;</g:HTML>
			<g:Button ui:field="saveSetButton">Save Set</g:Button>
			<g:HTML>&nbsp;</g:HTML>
		</g:HorizontalPanel>
		<g:FlexTable ui:field="drivesListFlexTable"></g:FlexTable>
	</g:VerticalPanel>
</ui:UiBinder> 