<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.width {
			width: 600px;
			font-size: 12px;
		}
	</ui:style>
	<g:VerticalPanel styleName="{style.width}" >
		<g:HTML ui:field="dwvLabel" styleName="{style.width}"></g:HTML>
		<g:HTML ui:field="gnuText" styleName="{style.width}"></g:HTML>
		<g:HTML ui:field="gnuLink" styleName="{style.width}"></g:HTML>
		<g:HTML ui:field="otherLinks" styleName="{style.width}"></g:HTML>
